{
  "name": "application",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "netlify dev",
    "dev:tunnel": "node runWithTunnel.cjs",
    "dev:vite": "vite",
    "build": "tsc && vite build",
    "build:brain": "cd brain && tsc",
    "brain": "cd brain && tsc && yarn run:brain",
    "brain:test": "cd brain && tsc && yarn run:brain:test",
    "brain:test:seed": "cd brain && tsc && cd .. && node brain/dist/src/test/test.js",
    "brain:test:seed:character": "cd brain && tsc && cd .. && node brain/dist/src/test/characterTest.js",
    "run:brain": "node brain/dist/src/brain.js",
    "run:brain:test": "node brain/dist/src/loop.js",
    "preview": "vite preview",
    "lint": "eslint --max-warnings 0 \"**/*.{ts,tsx,js,jsx}\"",
    "fix": "eslint --max-warnings 0 --fix \"**/*.{ts,tsx,js,jsx}\"",
    "types": "supabase gen types typescript --local | tee {brain/,}src/supabaseTypes.d.ts > /dev/null",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.0.0",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@netlify/functions": "^1.3.0",
    "@sentry/react": "^7.49.0",
    "@sentry/serverless": "^7.49.0",
    "@supabase/supabase-js": "^2.1.1",
    "axios": "^1.3.5",
    "axios-retry": "^3.8.0",
    "eslint-plugin-require-extensions": "^0.1.3",
    "framer-motion": "^6",
    "jsonschema": "^1.4.1",
    "leaflet": "^1.9.3",
    "moment": "^2.29.4",
    "ngrok": "^4.3.3",
    "node-steam-openid": "^1.0.1",
    "pg-boss": "^8.4.2",
    "postgres": "^3.3.4",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.43.9",
    "react-icons": "^4.8.0",
    "react-leaflet": "^4.2.1",
    "react-query": "^3.39.3",
    "react-router-dom": "^6",
    "react-supabase": "^0.2.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@emotion/eslint-plugin": "^11.7.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@testing-library/react-hooks": "^8.0.0",
    "@testing-library/user-event": "^14.2.0",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/leaflet": "^1.9.3",
    "@types/node-steam-openid": "^1.0.0",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "@vitejs/plugin-react": "^2.2.0",
    "@vitest/ui": "^0.25",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-chakra-ui": "^0.6.3",
    "eslint-plugin-import": "^2.24.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "eslint-plugin-sonarjs": "^0.13.0",
    "eslint-plugin-unicorn": "^41.0.1",
    "jsdom": "^19.0.0",
    "netlify-cli": "^12.2.10",
    "prettier": "^2.6.1",
    "supabase": "^1.50.9",
    "typescript": "^4.6.3",
    "vite": "^3",
    "vitest": "^0.25"
  }
